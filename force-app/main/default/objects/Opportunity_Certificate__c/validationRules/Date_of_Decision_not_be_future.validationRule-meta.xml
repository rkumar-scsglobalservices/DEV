<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_of_Decision_not_be_future</fullName>
    <active>true</active>
    <errorConditionFormula>AND(Date_of_Decision__c  &gt; TODAY(),$Profile.Name != &apos;SCS Integration&apos;)</errorConditionFormula>
    <errorDisplayField>Date_of_Decision__c</errorDisplayField>
    <errorMessage>Date of Decision should be either Today or a Past date. It can not be a future date.</errorMessage>
</ValidationRule>
