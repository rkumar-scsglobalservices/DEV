<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_of_Issuing_not_be_future</fullName>
    <active>false</active>
    <errorConditionFormula>AND( Date_of_Issuing__c  &gt; TODAY(),$Profile.Name != &apos;SCS Integration&apos;)</errorConditionFormula>
    <errorDisplayField>Date_of_Issuing__c</errorDisplayField>
    <errorMessage>Date of Issuing should be either Today or a Past date. It can not be a future date.</errorMessage>
</ValidationRule>
