<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>fw1__Total_Discount_Amount__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>((( fw1__Quantity__c  -  fw1__Discount_Quantity__c ) *  fw1__Unit_Price__c ) *  fw1__Discount_Percentage__c)
+
fw1__Discount_Amount__c</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Total Discount Amount</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
