<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>fw1__Total_Recurring_Amount__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>ROUND(
fw1__Net_Recurring_Amount__c  + 
fw1__Tax_Recurring_Amount__c  +  
fw1__Shipping_And_Handling__c,2)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Displays total recurring amount. This excludes non-recurring line items.</inlineHelpText>
    <label>Total Recurring Amount</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
