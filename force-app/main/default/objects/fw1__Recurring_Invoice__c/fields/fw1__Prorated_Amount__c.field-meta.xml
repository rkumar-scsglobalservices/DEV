<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>fw1__Prorated_Amount__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>IF( fw1__Add_Prorated_Amount_On_Next_Invoice__c, 
    (fw1__Next_Invoice_Date__c -  fw1__Start_Date__c) * fw1__Daily_Rate__c , 
    0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Prorated amount from Start Date to Next Invoice Date</inlineHelpText>
    <label>Prorated Amount</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
