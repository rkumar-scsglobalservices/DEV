<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Required_Incurred_by_Resource_or_User</fullName>
    <active>true</active>
    <errorConditionFormula>AND( 
ISBLANK(Incurred_by_User__c), ISBLANK( Incurred_by_Resource__c ) 
)</errorConditionFormula>
    <errorMessage>Incurred by Resource or Incurred by User must contain a value</errorMessage>
</ValidationRule>
