<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>inspire1__Percent_Overdue__c</fullName>
    <deprecated>false</deprecated>
    <description>Calculates the percentage the project is overdue (Total Days Overdue / Project Duration)</description>
    <externalId>false</externalId>
    <formula>IF (inspire1__Project_Duration__c &gt; 0, inspire1__Days_Overdue_Project__c /  inspire1__Project_Duration__c , NULL)</formula>
    <label>% Overdue</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
