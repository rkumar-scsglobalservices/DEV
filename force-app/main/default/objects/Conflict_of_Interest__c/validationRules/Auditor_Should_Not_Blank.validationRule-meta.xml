<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Auditor_Should_Not_Blank</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISBLANK( Auditor__c ) , ISBLANK( Auditor_Internal__c ) )</errorConditionFormula>
    <errorMessage>Internal or external auditor should not be blank</errorMessage>
</ValidationRule>
