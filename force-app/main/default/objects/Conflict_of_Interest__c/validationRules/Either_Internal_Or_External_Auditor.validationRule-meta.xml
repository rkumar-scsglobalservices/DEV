<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Either_Internal_Or_External_Auditor</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK( Auditor__c )),NOT(ISBLANK( Auditor_Internal__c )))</errorConditionFormula>
    <errorMessage>Select only one internal or external auditor at a time.</errorMessage>
</ValidationRule>
