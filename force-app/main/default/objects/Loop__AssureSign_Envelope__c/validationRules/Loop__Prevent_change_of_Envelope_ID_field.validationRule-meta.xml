<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Loop__Prevent_change_of_Envelope_ID_field</fullName>
    <active>true</active>
    <description>Prevent change of Envelope ID field</description>
    <errorConditionFormula>AND(PRIORVALUE(Loop__Envelope_ID__c) != NULL , ISCHANGED(Loop__Envelope_ID__c))</errorConditionFormula>
    <errorMessage>The Nintex AssureSign Envelope ID cannot be changed</errorMessage>
</ValidationRule>
